<div <% if(it.id) { %>id="<%= it.id %>"<% } %> class="profile-item<% if(it.classes) { %> <%= it.classes %><% } %>">
<% if(it.label && !['facebook', 'twitter', 'linkedin'].includes(it.label)) { %>
   <label class="item__key">
        <%= it.label %>
    </label>
<% } else { %>
    <a href="<%= it.url %>" target="_blank"><i class="icon <%= it.label %>"></i></a>
<% } %>
<% if(it.value) { %>
     <span class="item__value">
        <%= it.value %>
    </span>
<% } %>
</div>